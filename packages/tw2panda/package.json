{
  "name": "tw2panda",
  "type": "module",
  "version": "0.3.1",
  "bin": {
    "tw2panda": "bin.js"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.js"
    },
    "./config": {
      "types": "./dist/config.d.ts",
      "require": "./dist/config.cjs",
      "import": "./dist/config.js"
    },
    "./package.json": "./package.json"
  },
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/astahmer/tw2panda.git",
    "directory": "packages/tw2panda"
  },
  "scripts": {
    "start": "node ./dist/cli.cjs",
    "dev": "tsup --watch",
    "build": "tsup",
    "test": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@pandacss/config": "^0.37.2",
    "@pandacss/generator": "^0.37.2",
    "@pandacss/node": "^0.37.2",
    "@pandacss/parser": "^0.37.2",
    "@pandacss/preset-base": "^0.37.2",
    "@pandacss/preset-panda": "^0.37.2",
    "@pandacss/shared": "^0.37.2",
    "@pandacss/types": "^0.37.2",
    "bundle-n-require": "^1.0.1",
    "cac": "^6.7.14",
    "escalade": "^3.1.1",
    "hookable": "^5.5.3",
    "magic-string": "^0.30.1",
    "pathe": "^1.1.1",
    "postcss": "^8.4.26",
    "prettier": "2.8.4",
    "tailwindcss": "^3.3.3",
    "ts-morph": "21.0.1",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@pandacss/core": "^0.37.2",
    "@pandacss/token-dictionary": "^0.37.2",
    "@types/node": "^20.4.2",
    "@types/prettier": "2.7.3",
    "rimraf": "^5.0.1",
    "tsup": "8.0.2",
    "typescript": "^5.1.6",
    "vite-node": "^0.33.0",
    "vitest": "^0.33.0"
  },
  "files": [
    "bin.js",
    "config.d.ts",
    "src",
    "dist",
    "cli",
    "README.md"
  ],
  "keywords": [
    "panda",
    "css",
    "pandacss",
    "cli",
    "tailwind",
    "converter",
    "migration"
  ],
  "sideEffects": false,
  "publishConfig": {
    "access": "public"
  }
}
