{
  "publisher": "astahmer",
  "name": "tw2panda-vscode",
  "displayName": "tw2panda üêº",
  "description": "Easily migrate code from tailwind to Panda CSS",
  "version": "0.5.1",
  "private": true,
  "engines": {
    "vscode": "^1.67.0"
  },
  "categories": [
    "Programming Languages",
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "icon": "assets/logo.png",
  "license": "MIT",
  "keywords": [
    "css",
    "pandacss",
    "design tokens",
    "documentation",
    "framework",
    "design system",
    "design",
    "tokens",
    "panda",
    "semantic",
    "w3c"
  ],
  "main": "dist/index.js",
  "files": [
    "dist",
    "assets",
    "package.json",
    "README.md",
    "LICENSE"
  ],
  "preview": true,
  "contributes": {
    "commands": [
      {
        "command": "tw2panda-vscode.rewrite-tw-selection",
        "title": "Rewrite tw to Panda CSS",
        "category": "tw2panda"
      }
    ]
  },
  "repository": {
    "url": "https://github.com/astahmer/tw2panda",
    "directory": "packages/vscode"
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch src/index.ts",
    "clean": "pnpm rimraf dist node_modules",
    "clean:windows": "node ./scripts/clean.cjs",
    "typecheck": "tsc --noEmit",
    "rewrite-deps": "tsx ./scripts/rewrite-workspace-dependencies.ts",
    "override-esbuild": "tsx ./scripts/override-esbuild-with-wasm.ts",
    "release": "pnpm build --silent && tsx ./scripts/publish.ts",
    "local:package": "VSCE_RELEASE_TYPE=rc pnpm release",
    "local:build": "pnpm rewrite-deps && pnpm clean && pnpm override-esbuild && pnpm i --ignore-scripts --ignore-workspace && pnpm local:package"
  },
  "dependencies": {
    "@pandacss/config": "^0.37.2",
    "@pandacss/generator": "^0.37.2",
    "@pandacss/node": "^0.37.2",
    "@pandacss/parser": "^0.37.2",
    "@pandacss/preset-base": "0.37.2",
    "@pandacss/preset-panda": "0.37.2",
    "@pandacss/shared": "^0.37.2",
    "prettier": "2.8.4",
    "tailwindcss": "^3.3.3",
    "ts-morph": "21.0.1",
    "tw2panda": "workspace:*"
  },
  "bundledDependencies": [
    "@pandacss/config",
    "@pandacss/generator",
    "@pandacss/extractor",
    "@pandacss/node",
    "@pandacss/parser",
    "@pandacss/preset-base",
    "@pandacss/preset-panda",
    "@pandacss/shared",
    "pathe",
    "prettier",
    "tailwindcss",
    "ts-morph",
    "tw2panda"
  ],
  "devDependencies": {
    "@pandacss/vsix-builder": "0.18.0",
    "@pnpm/find-workspace-dir": "^6.0.2",
    "@pnpm/find-workspace-packages": "^6.0.9",
    "@pnpm/types": "^9.2.0",
    "@types/dotenv": "^8.2.0",
    "@types/node": "20.4.1",
    "@types/rimraf": "^4.0.5",
    "@types/semver": "^7.5.0",
    "@types/vscode": "^1.67.0",
    "@vscode/vsce": "^2.20.0",
    "dotenv": "^16.3.1",
    "fast-glob": "^3.3.1",
    "fs-extra": "11.1.1",
    "pathe": "1.1.1",
    "perfect-debounce": "^1.0.0",
    "rimraf": "^5.0.1",
    "semver": "^7.5.4",
    "tsup": "8.0.2",
    "tsx": "4.11.0",
    "typescript": "^5.1.6",
    "vscode": "^1.1.37"
  },
  "overrides": {
    "esbuild": "npm:esbuild-wasm@latest"
  }
}
