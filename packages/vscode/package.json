{
  "publisher": "astahmer",
  "name": "tw2panda-vscode",
  "displayName": "tw2panda üêº",
  "description": "Easily migrate code from tailwind to Panda CSS",
  "version": "0.0.1",
  "private": true,
  "engines": {
    "vscode": "^1.67.0"
  },
  "categories": [
    "Programming Languages",
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "icon": "assets/logo.png",
  "license": "MIT",
  "keywords": [
    "css",
    "pandacss",
    "design tokens",
    "documentation",
    "framework",
    "design system",
    "design",
    "tokens",
    "panda",
    "semantic",
    "w3c"
  ],
  "main": "dist/index.js",
  "files": [
    "dist",
    "assets",
    "package.json",
    "README.md",
    "LICENSE"
  ],
  "preview": true,
  "contributes": {
    "commands": [
      {
        "command": "tw2panda-vscode.restart",
        "title": "Restart Panda Server",
        "category": "tw2panda"
      }
    ]
  },
  "repository": {
    "url": "https://github.com/chakra-ui/panda",
    "directory": "packages/extension/vscode"
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup src/index.ts",
    "clean": "rimraf dist node_modules",
    "typecheck": "tsc --noEmit",
    "rewrite-deps": "tsx ./scripts/rewrite-workspace-dependencies.ts",
    "override-esbuild": "tsx ./scripts/override-esbuild-with-wasm.ts",
    "release": "pnpm build --silent && tsx ./scripts/publish.ts",
    "local:build": "pnpm rewrite-deps && pnpm clean && pnpm override-esbuild && npm i --ignore-scripts && pnpm build && pnpm vsix-builder package"
  },
  "dependencies": {
    "@pandacss/dev": "0.7.0",
    "@pandacss/preset-base": "0.7.0",
    "@pandacss/preset-panda": "0.7.0",
    "pathe": "1.1.1",
    "tw2panda": "workspace:^"
  },
  "bundledDependencies": [
    "@pandacss/dev",
    "@pandacss/preset-base",
    "@pandacss/preset-panda",
    "pathe"
  ],
  "devDependencies": {
    "@pandacss/language-server": "0.7.0",
    "@pandacss/vsix-builder": "0.7.0",
    "@pnpm/find-workspace-dir": "^6.0.2",
    "@pnpm/find-workspace-packages": "^6.0.9",
    "@types/node": "20.4.1",
    "@types/vscode": "^1.80.0",
    "@vscode/vsce": "^2.19.0",
    "dotenv": "^16.3.1",
    "execa": "7.1.1",
    "fs-extra": "11.1.1",
    "semver": "^7.5.4",
    "tsup": "7.1.0",
    "typescript": "^5.1.6",
    "vscode": "^1.1.37",
    "vscode-languageclient": "^8.1.0"
  }
}
